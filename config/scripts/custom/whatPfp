(function(){
	const command = createCommand("whatpfp")

	command.expect(argType.User)

	command.execute( function(event) {
		const userPfp = event.args[0].effectiveAvatarUrl
		const reverseUrl = "https://www.google.com/searchbyimage?&image_url="
		event.respond("<" + reverseUrl + userPfp + ">")
	});
	help.add("whatpfp", "Returns the reverse image url of a users profile picture.", "moderation", "{user}", "@Fox")
})();
